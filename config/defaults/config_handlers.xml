<?xml version="1.0" encoding="UTF-8"?>
<ae:configurations xmlns:ae="http://youds.com/youds/config/global/envelope/1.1" xmlns="http://youds.com/youds/config/parts/config_handlers/1.1">
	<ae:configuration>
		<handlers>
			<!--
				+~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~+
				| Core YoudsFramework configuration handlers.                                         |
				| ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ |
				| Only 1 instance of AutoloadConfigHandler can be used in the application.   |
				| If you need to add classes to be autoloaded, add them to the               |
				| config/autoload.xml configuration file.                                    |
				+~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~+
			-->
			<handler pattern="%core.config_dir%/autoload.xml" class="YoudsFrameworkAutoloadConfigHandler">
				<validation>%core.youds_dir%/config/xsd/autoload.xsd</validation>
				<!-- 0.11 to 1.0 -->
				<transformation>%core.youds_dir%/config/xsl/autoload.xsl</transformation>
				<!-- 1.0 to 1.1 -->
				<transformation>%core.youds_dir%/config/xsl/autoload.xsl</transformation>
			</handler>
			<handler pattern="%core.system_config_dir%/autoload.xml" class="YoudsFrameworkAutoloadConfigHandler">
				<validation>%core.youds_dir%/config/xsd/autoload.xsd</validation>
				<!-- 0.11 to 1.0 -->
				<transformation>%core.youds_dir%/config/xsl/autoload.xsl</transformation>
				<!-- 1.0 to 1.1 -->
				<transformation>%core.youds_dir%/config/xsl/autoload.xsl</transformation>
			</handler>

			<handler pattern="%core.config_dir%/compile.xml" class="YoudsFrameworkCompileConfigHandler">
				<validation>%core.youds_dir%/config/xsd/compile.xsd</validation>
				<!-- 0.11 to 1.0 -->
				<transformation>%core.youds_dir%/config/xsl/compile.xsl</transformation>
				<!-- 1.0 to 1.1 -->
				<transformation>%core.youds_dir%/config/xsl/compile.xsl</transformation>
			</handler>
			<handler pattern="%core.system_config_dir%/compile.xml" class="YoudsFrameworkCompileConfigHandler">
				<validation>%core.youds_dir%/config/xsd/compile.xsd</validation>
				<!-- 0.11 to 1.0 -->
				<transformation>%core.youds_dir%/config/xsl/compile.xsl</transformation>
				<!-- 1.0 to 1.1 -->
				<transformation>%core.youds_dir%/config/xsl/compile.xsl</transformation>
			</handler>

			<handler pattern="%core.config_dir%/settings.xml" class="YoudsFrameworkSettingConfigHandler">
				<validation>%core.youds_dir%/config/xsd/settings.xsd</validation>
				<!-- 0.11 to 1.0 -->
				<transformation>%core.youds_dir%/config/xsl/settings.xsl</transformation>
				<!-- 1.0 to 1.1 -->
				<transformation>%core.youds_dir%/config/xsl/settings.xsl</transformation>
			</handler>

			<handler pattern="%core.config_dir%/factories.xml" class="YoudsFrameworkFactoryConfigHandler">
				<validation>%core.youds_dir%/config/xsd/factories.xsd</validation>
				<!-- 0.11 to 1.0 -->
				<transformation>%core.youds_dir%/config/xsl/factories.xsl</transformation>
				<!-- 1.0 to 1.1 -->
				<transformation>%core.youds_dir%/config/xsl/factories.xsl</transformation>
			</handler>

			<handler pattern="%core.config_dir%/databases.xml" class="YoudsFrameworkDatabaseConfigHandler">
				<validation>%core.youds_dir%/config/xsd/databases.xsd</validation>
				<!-- 0.11 to 1.0 -->
				<transformation>%core.youds_dir%/config/xsl/databases.xsl</transformation>
				<!-- 1.0 to 1.1 -->
				<transformation>%core.youds_dir%/config/xsl/databases.xsl</transformation>
			</handler>

			<handler pattern="%core.config_dir%/action_filters.xml" class="YoudsFrameworkFilterConfigHandler">
				<validation>%core.youds_dir%/config/xsd/filters.xsd</validation>
				<!-- 0.11 to 1.0 -->
				<transformation>%core.youds_dir%/config/xsl/filters.xsl</transformation>
				<!-- 1.0 to 1.1 -->
				<transformation>%core.youds_dir%/config/xsl/filters.xsl</transformation>
			</handler>

			<handler pattern="%core.config_dir%/global_filters.xml" class="YoudsFrameworkFilterConfigHandler">
				<validation>%core.youds_dir%/config/xsd/filters.xsd</validation>
				<!-- 0.11 to 1.0 -->
				<transformation>%core.youds_dir%/config/xsl/filters.xsl</transformation>
				<!-- 1.0 to 1.1 -->
				<transformation>%core.youds_dir%/config/xsl/filters.xsl</transformation>
			</handler>

			<handler pattern="%core.config_dir%/output_types.xml" class="YoudsFrameworkOutputTypeConfigHandler">
				<validation>%core.youds_dir%/config/xsd/output_types.xsd</validation>
				<!-- 0.11 to 1.0 -->
				<transformation>%core.youds_dir%/config/xsl/output_types.xsl</transformation>
				<!-- 1.0 to 1.1 -->
				<transformation>%core.youds_dir%/config/xsl/output_types.xsl</transformation>
			</handler>

			<handler pattern="%core.config_dir%/logging.xml" class="YoudsFrameworkLoggingConfigHandler">
				<validation>%core.youds_dir%/config/xsd/logging.xsd</validation>
				<!-- 0.11 to 1.0 -->
				<transformation>%core.youds_dir%/config/xsl/logging.xsl</transformation>
				<!-- 1.0 to 1.1 -->
				<transformation>%core.youds_dir%/config/xsl/logging.xsl</transformation>
			</handler>

			<handler pattern="%core.config_dir%/rbac_definitions.xml" class="YoudsFrameworkRbacDefinitionConfigHandler">
				<validation>%core.youds_dir%/config/xsd/rbac_definitions.xsd</validation>
				<!-- 0.11 to 1.0 -->
				<transformation>%core.youds_dir%/config/xsl/rbac_definitions.xsl</transformation>
				<!-- 1.0 to 1.1 -->
				<transformation>%core.youds_dir%/config/xsl/rbac_definitions.xsl</transformation>
			</handler>

			<handler pattern="%core.config_dir%/routing.xml" class="YoudsFrameworkRoutingConfigHandler">
				<validation>%core.youds_dir%/config/xsd/routing.xsd</validation>
				<!-- 0.11 to 1.0 -->
				<transformation>%core.youds_dir%/config/xsl/routing.xsl</transformation>
				<!-- 1.0 to 1.1 -->
				<transformation>%core.youds_dir%/config/xsl/routing.xsl</transformation>
			</handler>

			<handler pattern="%core.youds_dir%/routing/soap/wsdl.xml" class="YoudsFrameworkWsdlConfigHandler">
				<!-- 0.11 to 1.0 -->
				<transformation>%core.youds_dir%/config/xsl/routing.xsl</transformation>
				<!-- 1.0 to 1.1 -->
				<transformation>%core.youds_dir%/config/xsl/routing.xsl</transformation>
				<transformation for="compilation">%core.youds_dir%/routing/soap/wsdl.xsl</transformation>
			</handler>

			<handler pattern="%core.config_dir%/translation.xml" class="YoudsFrameworkTranslationConfigHandler">
				<validation>%core.youds_dir%/config/xsd/translation.xsd</validation>
				<!-- 0.11 to 1.0 -->
				<transformation>%core.youds_dir%/config/xsl/translation.xsl</transformation>
				<!-- 1.0 to 1.1 -->
				<transformation>%core.youds_dir%/config/xsl/translation.xsl</transformation>
			</handler>

			<handler pattern="%core.testing_dir%/config/suites.xml" class="YoudsFrameworkTestSuitesConfigHandler">
				<!-- <validation>%core.youds_dir%/config/xsd/module.xsd</validation>-->
				<!-- 1.0 to 1.1 -->
				<transformation>%core.youds_dir%/config/xsl/testing.suites.xsl</transformation> 
			</handler>

			<!--
				+~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~+
				| Global YoudsFramework configuration handlers.                                       |
				| ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ |
				| These handlers must not be registered with a relative path, because they   |
				| need to be associated with module configuration files of the same base     |
				| name. DO NOT register relative path configuration files with these base    |
				| names or the system may cease to function.                                 |
				+~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~+
			-->
			<handler pattern="%core.module_dir%/*/config/autoload.xml" class="YoudsFrameworkAutoloadConfigHandler">
				<validation>%core.youds_dir%/config/xsd/autoload.xsd</validation>
				<!-- 0.11 to 1.0 -->
				<transformation>%core.youds_dir%/config/xsl/autoload.xsl</transformation>
				<!-- 1.0 to 1.1 -->
				<transformation>%core.youds_dir%/config/xsl/autoload.xsl</transformation>
			</handler>

			<handler pattern="%core.module_dir%/*/config/action_filters.xml" class="YoudsFrameworkFilterConfigHandler">
				<validation>%core.youds_dir%/config/xsd/filters.xsd</validation>
				<!-- 0.11 to 1.0 -->
				<transformation>%core.youds_dir%/config/xsl/filters.xsl</transformation>
				<!-- 1.0 to 1.1 -->
				<transformation>%core.youds_dir%/config/xsl/filters.xsl</transformation>
			</handler>

			<handler pattern="%core.module_dir%/*/config/module.xml" class="YoudsFrameworkModuleConfigHandler">
				<validation>%core.youds_dir%/config/xsd/module.xsd</validation>
				<!-- 0.11 to 1.0 -->
				<transformation>%core.youds_dir%/config/xsl/module.xsl</transformation>
				<!-- 1.0 to 1.1 -->
				<transformation>%core.youds_dir%/config/xsl/module.xsl</transformation>
			</handler>

			<handler pattern="%core.module_dir%/*/validate/*.xml" class="YoudsFrameworkValidatorConfigHandler">
				<validation>%core.youds_dir%/config/xsd/validators.xsd</validation>
				<!-- 0.11 to 1.0 -->
				<transformation>%core.youds_dir%/config/xsl/validators.xsl</transformation>
				<!-- 1.0 to 1.1 -->
				<transformation>%core.youds_dir%/config/xsl/validators.xsl</transformation>
			</handler>

			<handler pattern="%core.module_dir%/*/cache/*.xml" class="YoudsFrameworkCachingConfigHandler">
				<validation>%core.youds_dir%/config/xsd/caching.xsd</validation>
				<!-- 0.11 to 1.0 -->
				<transformation>%core.youds_dir%/config/xsl/caching.xsl</transformation>
				<!-- 1.0 to 1.1 -->
				<transformation>%core.youds_dir%/config/xsl/caching.xsl</transformation>
			</handler>

			<handler pattern="%core.cldr_dir%/locales/*.xml" class="YoudsFrameworkLdmlConfigHandler" />
			<handler pattern="%core.cldr_dir%/supplementalData.xml" class="YoudsFrameworkLdmlSupplementalConfigHandler" />
		</handlers>
	</ae:configuration>
</ae:configurations>
