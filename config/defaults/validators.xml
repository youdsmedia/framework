<?xml version="1.0" encoding="utf-8"?>
<ae:configurations xmlns="http://youds.com/youds/config/parts/validators/1.1" xmlns:ae="http://youds.com/youds/config/global/envelope/1.1">
	
	<ae:configuration>
		<validator_definitions>
			
			<validator_definition name="and" class="YoudsFrameworkAndoperatorValidator">
				<ae:parameter name="break">true</ae:parameter>
			</validator_definition>
			
			<validator_definition name="arraylength" class="YoudsFrameworkArraylengthValidator">
				<ae:parameter name="min">1</ae:parameter>
			</validator_definition>
			
			<validator_definition name="boolean" class="YoudsFrameworkBooleanValidator" />
			
			<validator_definition name="datetime" class="YoudsFrameworkDateTimeValidator">
				<ae:parameter name="check">true</ae:parameter>
			</validator_definition>
			
			<validator_definition name="email" class="YoudsFrameworkEmailValidator" />
			
			<validator_definition name="equals" class="YoudsFrameworkEqualsValidator" />
			
			<validator_definition name="file" class="YoudsFrameworkFileValidator" />
			
			<validator_definition name="imagefile" class="YoudsFrameworkImageFileValidator" />
			
			<validator_definition name="inarray" class="YoudsFrameworkInarrayValidator">
				<ae:parameter name="sep">,</ae:parameter>
			</validator_definition>
			
			<validator_definition name="isset" class="YoudsFrameworkIssetValidator" />
			
			<validator_definition name="isnotempty" class="YoudsFrameworkIsNotEmptyValidator" />

			<validator_definition name="json" class="YoudsFrameworkJsonValidator" />
			
			<validator_definition name="not" class="YoudsFrameworkNotoperatorValidator" />
			
			<validator_definition name="number" class="YoudsFrameworkNumberValidator">
				<ae:parameter name="type">int</ae:parameter>
			</validator_definition>
			
			<validator_definition name="or" class="YoudsFrameworkOroperatorValidator">
				<ae:parameter name="break">true</ae:parameter>
			</validator_definition>
			
			<validator_definition name="regex" class="YoudsFrameworkRegexValidator">
				<ae:parameter name="match">true</ae:parameter>
			</validator_definition>
			
			<validator_definition name="set" class="YoudsFrameworkSetValidator" />
			
			<validator_definition name="string" class="YoudsFrameworkStringValidator">
				<ae:parameter name="min">1</ae:parameter>
			</validator_definition>
			
			<validator_definition name="xor" class="YoudsFrameworkXoroperatorValidator" />
			
		</validator_definitions>
	</ae:configuration>
	
</ae:configurations>
